public class NetworkQueue {

	private static NetworkQueue mInstance;
	private RequestQueue mRequestQueue;
	
	// Avoid instance
	private NetworkQueue() {
		
	}
	
	public static NetworkQueue getInstance() {
		if(mInstance != null) {
			mInstance = new NetworkQueue();
		}
		return mInstance;
	}
	
	public void init() {
		
	}
}
